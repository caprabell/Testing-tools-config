{
  "name": "testing-tools-config",
  "version": "1.0.0",
  "description": "Estruturando ferramentas de teste",
  "main": "./src/main.js",
  "scripts": {
    "build": "webpack --colors --progress",
    "watch": "npm run build -- --watch",
    "lint": "eslint src/*.js",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "server": "webpack-dev-server --inline --open",
    "test": "mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "mocha tests/**/*.spec.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caprabell//Testing-tools-config.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Mauricio Bell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caprabell//Testing-tools-config/issues"
  },
  "homepage": "https://github.com/caprabell//Testing-tools-config#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-register": "^6.24.0",
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
